{
  "name": "lscript-syntax",
  "displayName": "LScript Syntax Highlighting",
  "description": "Syntax highlighting for LScript",
  "version": "0.0.1",
  "publisher": "Lucas",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "lscript",
        "aliases": ["LScript", "lscript"],
        "extensions": [".lcs"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lscript",
        "scopeName": "source.lscript",
        "path": "./syntaxes/lscript.tmLanguage.json"
      }
    ]
  }
}